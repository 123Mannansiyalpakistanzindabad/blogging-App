                <div class="container mb-3">
                    <label for="body">Body</label>
                    <textarea class="form-control" name="body" id="body"></textarea>
                </div>
                <div class="container mb-3">
                    <button class="btn btn-info">Submit</button>
                </div>
            </form>
</body>

</html>